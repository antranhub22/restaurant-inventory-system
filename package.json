{
  "name": "restaurant-inventory-system",
  "version": "1.0.0",
  "description": "Hệ thống quản lý kho nhà hàng",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && cp -r src/types dist/",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "setup": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-editor-ajrm": "^2.5.14",
    "react-router-dom": "^6.14.2",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "tesseract.js": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-json-editor-ajrm": "^2.5.3",
    "@types/socket.io": "^3.0.2",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
