{
  "name": "backend",
  "version": "1.0.0",
  "description": "Restaurant Inventory Management System Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run build:verify",
    "build:verify": "test -f dist/server.js && echo '✅ Build verification: dist/server.js exists' || (echo '❌ Build failed: dist/server.js missing' && exit 1)",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "start:prod": "npm run build && npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:check": "node check-database.js",
    "db:setup": "npx prisma db push && npm run db:seed",
    "setup:admin": "node setup-admin.js",
    "db:debug": "node debug-database.js",
    "health:check": "curl -f http://localhost:${PORT:-4000}/api/health || exit 1",
    "verify:all": "npm run build:verify && npm run db:check"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^5.2.0",
    "@prisma/client": "^5.9.1",
    "@types/ioredis": "^4.28.10",
    "@types/redis": "^4.0.10",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "fastest-levenshtein": "^1.0.16",
    "fuse.js": "^7.1.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.8.2",
    "prisma": "^5.9.1",
    "rate-limit-redis": "^4.2.1",
    "redis": "^5.5.6",
    "sharp": "^0.34.2",
    "tesseract.js": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/connect": "^3.4.38",
    "@types/connect-redis": "^0.0.23",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.6",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/serve-static": "^1.15.5",
    "@types/supertest": "^2.0.16",
    "@types/tesseract.js": "^0.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0"
  }
}
